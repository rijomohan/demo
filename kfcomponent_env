def loadProperties() {
    node {
        checkout scm
        properties = readProperties file: '/home/rijomohan/kfpipeline/jenkins_deploy/seldon/customenv.properties'
        
    }
}

pipeline {
    agent none

    stages {           
        stage ('prepare') {
            agent any

            steps {
                script {
                    loadProperties()
                    echo $DEV_PROJECTID
                }
            }
        }
	
	}
	}
